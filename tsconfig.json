{
	"compilerOptions": {
		"strict": true,
		"module": "ESNext",
		"target": "ESNext",
		"moduleResolution": "node",
		"outDir": "dist",
		"declarationDir": "dist",
		"declaration": true,
		"declarationMap": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"noImplicitAny": false,
		"strictNullChecks": false,
		"noImplicitThis": false,
		"strictPropertyInitialization": false,
		"skipLibCheck": true,
		"sourceMap": true,
		"paths": {
			"from-exponential": [
				"./node_modules/from-exponential"
			]
		}
	},
	"include": [
		"src"
	],
	"exclude": [
		"node_modules",
		"dist"
	],
	"types": [
		"@types/lodash",
		"@types/node"
	]
}